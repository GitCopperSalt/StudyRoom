mit_after","get_register","submit_sub","message","$post","$router","go","is_view","bl","$check_action","uploadimg","uploadFile","file","created"],"sources":["src/views/study_room_administrator/view.vue"],"sourcesContent":["<template>\n\t<el-main class=\"bg\">\n\t\t<el-form ref=\"form\" :model=\"form\" status-icon label-width=\"120px\" v-if=\"is_view()\">\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"头像\" prop=\"avatar\">\n\t\t\t\t\t<el-upload class=\"avatar-uploader\" drag accept=\"image/gif, image/jpeg, image/png, image/jpg\"\n\t\t\t\t\t\taction=\"\" :http-request=\"uploadimg\" :show-file-list=\"false\">\n\t\t\t\t\t\t<img v-if=\"form.avatar\" :src=\"$fullUrl(form.avatar)\" class=\"avatar\">\n\t\t\t\t\t\t<i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n\t\t\t\t\t</el-upload>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"账号\" prop=\"username\">\n\t\t\t        <span v-if=\"obj.username\">{{obj.username}}</span>\n\t\t\t\t\t<el-input v-else v-model=\"form.username\" placeholder=\"请输入账号\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"密码\" prop=\"password\">\n\t\t\t\t\t<span v-if=\"obj.password\">{{obj.password}}</span>\n\t\t\t        <el-input v-else type=\"password\" v-model=\"form.password\" placeholder=\"请输入密码\" show-password></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"昵称\" prop=\"nickname\">\n\t\t\t\t\t<el-input v-model=\"form.nickname\" placeholder=\"请输入昵称\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"手机号码\" prop=\"phone\">\n\t\t\t\t\t<el-input type=\"phone\" v-model=\"form.phone\" placeholder=\"请输入手机号码\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"邮箱\" prop=\"email\">\n\t\t\t\t\t<el-input type=\"email\" v-model=\"form.email\" placeholder=\"请输入邮箱\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"手机认证\" prop=\"phone_state\">\n\t\t\t\t\t<el-select v-model=\"form.phone_state\" placeholder=\"请选择\">\n\t\t\t\t\t\t<el-option v-for=\"group in list_state\" :key=\"group.value\" :label=\"group.name\"\n\t\t\t\t\t\t\t:value=\"group.value\">\n\t\t\t\t\t\t</el-option>\n\t\t\t\t\t</el-select>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"邮箱认证\" prop=\"email_state\">\n\t\t\t\t\t<el-select v-model=\"form.email_state\" placeholder=\"请选择\">\n\t\t\t\t\t\t<el-option v-for=\"group in list_state\" :key=\"group.value\" :label=\"group.name\"\n\t\t\t\t\t\t\t:value=\"group.value\">\n\t\t\t\t\t\t</el-option>\n\t\t\t\t\t</el-select>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"状态\" prop=\"state\">\n\t\t\t\t\t<el-select v-model=\"form.state\" placeholder=\"请选择\">\n\t\t\t\t\t\t<el-option v-for=\"group in list_user_state\" :key=\"group.value\" :label=\"group.name\"\n\t\t\t\t\t\t\t:value=\"group.value\">\n\t\t\t\t\t\t</el-option>\n\t\t\t\t\t</el-select>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','merchant_account_number') || $check_field('add','merchant_account_number') || $check_field('set','merchant_account_number')\" :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"商家账号\" prop=\"merchant_account_number\">\n\t\t\t\t\t<el-input id=\"merchant_account_number\" v-model=\"form_sub['merchant_account_number']\" placeholder=\"请输入商家账号\"\n\t\t\t\t\t\t\t  v-if=\"user_group === '管理员' || (form_sub['study_room_administrator_id'] && $check_field('set','merchant_account_number')) || (!form_sub['study_room_administrator_id'] && $check_field('add','merchant_account_number'))\"></el-input>\n\t\t\t\t\t<div v-else-if=\"$check_field('get','merchant_account_number')\">{{form_sub['merchant_account_number']}}</div>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','full_name') || $check_field('add','full_name') || $check_field('set','full_name')\" :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"姓名\" prop=\"full_name\">\n\t\t\t\t\t<el-input id=\"full_name\" v-model=\"form_sub['full_name']\" placeholder=\"请输入姓名\"\n\t\t\t\t\t\t\t  v-if=\"user_group === '管理员' || (form_sub['study_room_administrator_id'] && $check_field('set','full_name')) || (!form_sub['study_room_administrator_id'] && $check_field('add','full_name'))\"></el-input>\n\t\t\t\t\t<div v-else-if=\"$check_field('get','full_name')\">{{form_sub['full_name']}}</div>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','gender') || $check_field('add','gender') || $check_field('set','gender')\" :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item label=\"性别\" prop=\"gender\">\n\t\t\t\t\t<el-select id=\"gender\" v-model=\"form_sub['gender']\"\n\t\t\t\t\t\tv-if=\"user_group === '管理员' || (form_sub['study_room_administrator_id'] && $check_field('set','gender')) || (!form_sub['study_room_administrator_id'] && $check_field('add','gender'))\">\n\t\t\t\t\t\t<el-option v-for=\"o in list_gender\" :key=\"o\" :label=\"o\" :value=\"o\">\n\t\t\t\t\t\t</el-option>\n\t\t\t\t\t</el-select>\n\t\t\t\t\t<div v-else-if=\"$check_field('get','gender')\">{{form_sub['gender']}}</div>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\n\t\t\t\t<el-form-item>\n\t\t\t\t\t<el-button type=\"primary\" @click=\"submit()\">提交</el-button>\n\t\t\t\t\t<el-button @click=\"cancel()\">取消</el-button>\n\t\t\t\t</el-form-item>\n\t\t\t</el-col>\n\n\t\t</el-form>\n\t</el-main>\n</template>\n\n<script>\n\timport mixin from \"@/mixins/page.js\";\n\n\texport default {\n\t\tmixins: [mixin],\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tfield: \"user_id\",\n\t\t\t\turl_add: \"~/api/user/register?\",\n\t\t\t\turl_set: \"~/api/user/set?\",\n\t\t\t\turl_get_obj: \"~/api/user/get_obj?\",\n\t\t\t\turl_upload: \"~/api/user/upload?\",\n\n\t\t\t\tgroup_table: \"study_room_administrator\",\n\t\t\t\tis_password: true,\n\n\t\t\t\tquery: {\n\t\t\t\t\t\"user_id\": 0,\n\t\t\t\t},\n\n\t\t\t\tobj: {\n\t\t\t\t\tuser_id: 0,\n\t\t\t\t\tusername: '',\n\t\t\t\t\tnickname: '',\n\t\t\t\t\tpassword: '',\n\t\t\t\t\tavatar: '',\n\t\t\t\t\tphone: '',\n\t\t\t\t\temail: '',\n\t\t\t\t\tuser_group: \"自习室管理员\",\n\t\t\t\t\tphone_state: 0,\n\t\t\t\t\temail_state: 0,\n\t\t\t\t\tstate: 1\n\t\t\t\t},\n\n\t\t\t\tform: {\n\t\t\t\t\tuser_id: 0,\n\t\t\t\t    username: '',\n\t\t\t\t\tnickname: '',\n\t\t\t\t    password: '',\n\t\t\t\t\tavatar: '',\n\t\t\t\t\tphone: '',\n\t\t\t\t\temail: '',\n\t\t\t\t\tuser_group: \"自习室管理员\",\n\t\t\t\t\tphone_state: 0,\n\t\t\t\t\temail_state: 0,\n\t\t\t\t\tstate: 1\n\t\t\t\t},\n\n\t\t\t\tdisabledObj:{\n\t\t\t\t\t\"merchant_account_number_isDisabled\": false,\n\t\t\t\t\t\"full_name_isDisabled\": false,\n\t\t\t\t\t\"gender_isDisabled\": false,\n\t\t\t\t},\n\n\t\t\t\tform_sub: {\n\t\t\t\t\t\"merchant_account_number\":'', // 商家账号\n\t\t\t\t\t\"full_name\":'', // 姓名\n\t\t\t\t\t\"gender\":'', // 性别\n\t\t\t\t    \"user_id\": 0,\n\t\t\t\t\t\"study_room_administrator_id\": 0 // ID\n\t\t\t\t},\n\n\t\t\t\tlist_state: [{\n\t\t\t\t\tvalue: 0,\n\t\t\t\t\tname: \"未认证\"\n\t\t\t\t}, {\n\t\t\t\t\tvalue: 1,\n\t\t\t\t\tname: \"审核中\"\n\t\t\t\t}, {\n\t\t\t\t\tvalue: 2,\n\t\t\t\t\tname: \"已认证\"\n\t\t\t\t}],\n\n\t\t\t\tlist_user_state: [{\n\t\t\t\t\tvalue: 1,\n\t\t\t\t\tname: \"可用\"\n\t\t\t\t}, {\n\t\t\t\t\tvalue: 2,\n\t\t\t\t\tname: \"异常\"\n\t\t\t\t}, {\n\t\t\t\t\tvalue: 3,\n\t\t\t\t\tname: \"已冻结\"\n\t\t\t\t}, {\n\t\t\t\t\tvalue: 4,\n\t\t\t\t\tname: \"已注销\"\n\t\t\t\t}],\n\t\t\t\t//性别选项列表